<!DOCTYPE html>
<html>
  <head>
    <title>Add Numbers</title>
  </head>
  <body>
    <button onclick="addNumbers()">Add Numbers</button>

    <script>
      function addNumbers() {
        // Define the two numbers
        const num1 = 5;
        const num2 = 7;

        // Calculate the sum
        const sum = num1 + num2;

        // Show the result as an alert message
        alert("The sum is: " + sum);
      }
    </script>
  </body>
</html>